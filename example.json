{
	"repo": "/path/to/backup",
	"passwd": "password",
	"dry-run": false,
	"init": false,
	"entry": [
		{
			"tag": ["home", "data"],
			"source": ["${HOME}"],
			"keeplast": "NaN",	
			"exclude": ["**/.*",
						"**/*.o",
						"**/*.o.d",
						"**/.local"
						]
		},
		{
			"tag": ["config"],
			"source": ["/etc", "${HOME}/.config"],
			"sudo": true,
			"exclude": ["**/.*"]	
		}
	]
}